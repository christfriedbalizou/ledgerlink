<main class="px-4 py-6 sm:px-6 lg:px-8">
	<div class="max-w-5xl mx-auto space-y-6">
		<div class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-4 mb-10">
			<div class="card flex items-center justify-between">
				<div>
					<p class="text-sm font-medium text-gray-500 dark:text-gray-400">Sync Status</p>
					<p class="mt-1 text-lg font-semibold <%= syncStatus === 'Operational' ? 'text-green-600 dark:text-green-400' : 'text-yellow-600 dark:text-yellow-400' %>"><%= syncStatus %></p>
				</div>
				<svg class="h-8 w-8 text-green-600 dark:text-green-400" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"/></svg>
			</div>
			<div class="card flex items-center justify-between">
				<div>
					<p class="text-sm font-medium text-gray-500 dark:text-gray-400">Total Users</p>
					<p class="mt-1 text-lg font-semibold text-blue-600 dark:text-blue-400"><%= totalUsers %></p>
				</div>
				<svg class="h-8 w-8 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197m13.5-9a2.5 2.5 0 11-5 0 2.5 2.5 0 015 0z"/></svg>
			</div>
			<div class="card flex items-center justify-between">
				<div>
					<p class="text-sm font-medium text-gray-500 dark:text-gray-400">Total Institutions</p>
					<p class="mt-1 text-lg font-semibold text-purple-600 dark:text-purple-400"><%= totalInstitutions %></p>
				</div>
				<svg class="h-8 w-8 text-purple-600 dark:text-purple-400" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"/></svg>
			</div>
			<div class="card flex items-center justify-between">
				<div>
					<p class="text-sm font-medium text-gray-500 dark:text-gray-400">Total Accounts</p>
					<p class="mt-1 text-lg font-semibold text-yellow-600 dark:text-yellow-400"><%= totalAccounts %></p>
				</div>
				<svg class="h-8 w-8 text-yellow-600 dark:text-yellow-400" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 9V7a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2m2 4h10a2 2 0 002-2v-6a2 2 0 00-2-2H9a2 2 0 00-2 2v6a2 2 0 002 2zm7-5a2 2 0 11-4 0 2 2 0 014 0z"/></svg>
			</div>
		</div>

		<div class="grid">
			<div class="card relative pl-4 border-l-4 border-primary-500 dark:border-primary-400 group transition-colors">
                <div class="flex items-start justify-between">
                  <div class="flex items-center space-x-4">
                    <div>
                      <h3 class="text-base font-semibold text-gray-900 dark:text-gray-100 transition-colors">General Settings</h3>
                    </div>
                  </div>
                </div>
				<ul class="mt-5 divide-y divide-gray-200 dark:divide-gray-700 rounded-md">
					<% Object.entries(generalSettings || {}).forEach(([key, value])=> { %>
						<li class="p-2 flex items-center justify-between">
							<div class="space-y-1 w-full">
								<p class="text-sm font-medium text-gray-400">
									<%= key %> <span class="text-gray-500 pl-4 font-normal float-right"><%= value %></span>
								</p>
							</div>
						</li>
					<% }) %>
				</ul>
			</div>
		</div>

		<div class="grid">
			<div class="card relative pl-4 border-l-4 border-primary-500 dark:border-primary-400 group transition-colors">
                <div class="flex items-start justify-between">
                  <div class="flex items-center space-x-4">
                    <div>
                      <h3 class="text-base font-semibold text-gray-900 dark:text-gray-100 transition-colors">Plaid Settings</h3>
                    </div>
                  </div>
                </div>
				<ul class="mt-5 divide-y divide-gray-200 dark:divide-gray-700 rounded-md">
					<% Object.entries(plaidSettings || {}).forEach(([key, value])=> { %>
						<li class="p-2 flex items-center justify-between">
							<div class="space-y-1 w-full">
								<p class="text-sm font-medium text-gray-400">
									<%= key %> <span class="text-gray-500 pl-4 font-normal float-right"><%= value %></span>
								</p>
							</div>
						</li>
					<% }) %>
				</ul>
			</div>
		</div>
	</div>
</main>