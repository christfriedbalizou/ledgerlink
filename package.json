{
  "name": "project-scaffolding",
  "version": "1.0.0",
  "description": "Base project for OIDC/OAuth2 application setup.",
  "main": "server.js",
  "scripts": {
    "start": "npm run db:push && node server.js",
    "dev": "npm run db:push && nodemon server.js",
    "test": "npm run db:seed && node --experimental-vm-modules node_modules/.bin/jest",
    "db:generate": "node scripts/generate-schema.js && prisma generate",
    "db:migrate": "node scripts/generate-schema.js && prisma migrate dev --name init --create-only",
    "db:push": "node scripts/generate-schema.js && prisma db push",
    "db:seed": "npm run db:push && node src/db/seed.js"
  },
  "keywords": [
    "express",
    "oauth",
    "oidc",
    "docker"
  ],
  "author": "",
  "type": "module",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.2",
    "express": "^5.0.0",
    "express-session": "^1.18.2",
    "openid-client": "^6.8.0",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.16.2",
    "cross-env": "^10.0.0",
    "jest": "^30.1.3",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4"
  }
}
